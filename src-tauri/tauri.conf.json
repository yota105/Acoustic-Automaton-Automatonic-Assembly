{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Tauri Electronics Template",
  "version": "1.0.0",
  "identifier": "com.tauri-electronics.template",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "controller",
        "title": "Main Control",
        "url": "src/controller.html",
        "width": 800,
        "height": 700,
        "resizable": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
