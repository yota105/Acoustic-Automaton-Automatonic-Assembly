{
    "refId": "mysynth",
    "kind": "faust-precompiled",
    "category": "hybrid",
    "subCategory": "synth-processor",
    "label": "My Synthesizer",
    "description": "Sawtooth oscillator with microphone input mixing capability",
    "dspPath": "mysynth.dsp",
    "compatibility": {
        "canBeSource": true,
        "canBeInsert": false,
        "canBeSend": false,
        "requiresInput": false
    },
    "params": [
        {
            "id": "freq",
            "label": "Frequency",
            "addr": "/freq",
            "min": 50,
            "max": 1000,
            "default": 200,
            "unit": "Hz"
        },
        {
            "id": "gain",
            "label": "Gain",
            "addr": "/gain",
            "min": 0,
            "max": 0.5,
            "default": 0.5,
            "unit": ""
        },
        {
            "id": "input_mix",
            "label": "Input Mix",
            "addr": "/input_mix",
            "min": 0,
            "max": 1,
            "default": 0.5,
            "unit": ""
        }
    ],
    "tags": [
        "synthesizer",
        "mixer",
        "sawtooth",
        "microphone"
    ],
    "author": "User",
    "version": "1.0.0"
}