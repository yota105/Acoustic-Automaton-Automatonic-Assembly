{
    "refId": "mysynth",
    "kind": "faust-precompiled",
    "category": "hybrid",
    "subCategory": "synth-processor",
    "label": "My Synthesizer",
    "description": "Sawtooth oscillator with microphone input mixing capability",
    "dspPath": "mysynth.dsp",
    "compatibility": {
        "canBeSource": true,
        "canBeInsert": false,
        "canBeSend": false,
        "requiresInput": false
    },
    "params": [
        {
            "id": "freq",
            "label": "Frequency",
            "addr": "/mysynth/freq",
            "min": 50,
            "max": 2000,
            "default": 200,
            "unit": "Hz"
        },
        {
            "id": "gain",
            "label": "Gain",
            "addr": "/mysynth/gain",
            "min": 0,
            "max": 0.5,
            "default": 0.3,
            "unit": ""
        },
        {
            "id": "input_mix",
            "label": "Input Mix",
            "addr": "/mysynth/input_mix",
            "min": 0,
            "max": 1,
            "default": 0.5,
            "unit": ""
        },
        {
            "id": "env_attack",
            "label": "Env Attack",
            "addr": "/mysynth/env/attack",
            "min": 0.001,
            "max": 2.0,
            "default": 0.02,
            "unit": "s"
        },
        {
            "id": "env_decay",
            "label": "Env Decay",
            "addr": "/mysynth/env/decay",
            "min": 0.001,
            "max": 2.0,
            "default": 0.05,
            "unit": "s"
        },
        {
            "id": "env_sustain",
            "label": "Env Sustain",
            "addr": "/mysynth/env/sustain",
            "min": 0,
            "max": 1,
            "default": 0.8,
            "unit": ""
        },
        {
            "id": "env_release",
            "label": "Env Release",
            "addr": "/mysynth/env/release",
            "min": 0.005,
            "max": 3.0,
            "default": 0.4,
            "unit": "s"
        },
        {
            "id": "gate",
            "label": "Gate",
            "addr": "/mysynth/gate",
            "min": 0,
            "max": 1,
            "default": 1,
            "unit": ""
        }
    ],
    "tags": [
        "synthesizer",
        "mixer",
        "sawtooth",
        "microphone"
    ],
    "author": "User",
    "version": "1.0.0"
}